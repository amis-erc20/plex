{
  "name": "dharma",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dharmaprotocol/dharma.js": "^0.0.16",
    "@types/bignumber.js": "^5.0.0",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-jsonschema-form": "^1.0.1",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "3",
    "@types/react-router-redux": "^4",
    "@types/react-scroll": "^1.5.3",
    "@types/react-select": "^1.2.3",
    "@types/react-toggle": "4.0.1",
    "@types/reactstrap": "^5.0.17",
    "NonFungibleToken": "https://github.com/dharmaprotocol/NonFungibleToken",
    "abi-decoder": "^1.0.9",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^5.0.0",
    "bitly": "^5.0.12",
    "bootstrap": "^4.0.0",
    "dotenv": "^5.0.1",
    "identicon.js": "^2.3.1",
    "lodash.compact": "^3.0.1",
    "ngrok": "^2.2.26",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jsonschema-form": "^1.0.1",
    "react-redux": "^5.0.6",
    "react-router": "3",
    "react-router-redux": "^4.0.8",
    "react-scripts-ts": "2.13.0",
    "react-scroll": "^1.7.6",
    "react-select": "^1.2.1",
    "react-toggle": "4.0.1",
    "reactstrap": "5.0.0-beta",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.1.6",
    "tiny-promisify": "^1.0.0",
    "truffle": "4.0.3",
    "zeppelin-solidity": "^1.6.0"
  },
  "scripts": {
    "contracts:compile": "truffle compile",
    "contracts:migrate": "truffle migrate && cp build/contracts/* src/artifacts/",
    "chain": "ganache-cli -m 'pink proof foil skull tide narrow audit card rotate forget liar favorite'",
    "start": "react-scripts-ts start --history-api-fallback",
    "build": "react-scripts-ts build",
    "test:watch": "jest --watch --runInBand",
    "eject": "react-scripts-ts eject",
    "ngrok:start": "ngrok http 3000",
    "migrations:loans": "tsc; node build/dist/migrations/loans.js"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-enzyme": "^4.2.0",
    "redux-mock-store": "1.4.0",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.1",
    "web3-typescript-typings": "^0.9.11"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__test__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePaths": [
      "<rootDir>/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__test__/setup.js",
      "/build/dist/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "mapCoverage": true,
    "setupTestFrameworkScriptFile": "./__test__/setup.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ]
  }
}
